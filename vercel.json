{
  "version": 2,
  "builds": [
    {
      "src": "index.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/controllers/.*",
      "dest": "/controllers/authController.mjs"
    },
    {
      "src": "/controllers/.*",
      "dest": "/controllers/nutritionController.mjs"
    },
    {
      "src": "/controllers/.*",
      "dest": "/controllers/trainingController.mjs"
    },
    {
      "src": "/controllers/.*",
      "dest": "/controllers/uidController.mjs"
    },
    {
      "src": "/routes/.*",
      "dest": "/routes/authRoutes.mjs"
    },
    {
      "src": "/routes/.*",
      "dest": "/routes/nutritionRoutes.mjs"
    },
    {
      "src": "/routes/.*",
      "dest": "/routes/trainingRoutes.mjs"
    },
    {
      "src": "/routes/.*",
      "dest": "/routes/uidRoutes.mjs"
    },
    {
      "src": "/.*",
      "dest": "/index.mjs"
    }
  ]
}



