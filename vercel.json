{
  "version": 2,
  "builds": [
    {
      "src": "index.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth/.*",
      "dest": "/controllers/authController.mjs"
    },
    {
      "src": "/uid/.*",
      "dest": "/controllers/uidController.mjs"
    },
    {
      "src": "/nutrition/.*",
      "dest": "/controllers/nutritionController.mjs"
    },
    {
      "src": "/training/.*",
      "dest": "/controllers/trainingController.mjs"
    },
    {
      "src": "/.*",
      "dest": "/index.mjs"
    }
  ]
}

