{
  "version": 2,
  "builds": [
    {
      "src": "index.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/controllers/(.*)",
      "dest": "/controllers/$1"
    },
    {
      "src": "/routes/(.*)",
      "dest": "/routes/$1"
    },
    {
      "src": "/.*",
      "dest": "/index.mjs"
    }
  ]
}


